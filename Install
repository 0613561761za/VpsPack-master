#/bin/bash
rm /etc/localtime && ln -s /usr/share/zoneinfo/America/Sao_Paulo /etc/localtime
wget https://raw.githubusercontent.com/matheuslarcher20/VpsPack-master/master/configuracao && bash configuracao
wget https://raw.githubusercontent.com/matheuslarcher20/VpsPack-master/master/OpenVPN-install && bash OpenVPN-install
wget https://raw.githubusercontent.com/matheuslarcher20/VpsPack-master/master/VpsPack-install && bash VpsPack-install
rm configuracao /dev/null 2>/dev/null
rm OpenVPN-install /dev/null 2>/dev/null
rm Install /dev/null 2>/dev/null
rm VpsPack-install /dev/null 2>/dev/null
cache-detalhesusers
echo " Execute cache-detalhesusers.install em uma seção screen. "
echo -e "
 \033[1;32mInstalacao concluida!!\033[0m
 "
