#/bin/bash
clear
apt-get update -y && apt-get upgrade -y
clear
wget https://raw.githubusercontent.com/RicKbrL/VpsConf/master/config && bash config && wget https://raw.githubusercontent.com/matheuslarcher20/VpsPack-master/master/VpsPack-install && bash VpsPack-install
clear

echo -e "\033[1;37mConcluido"
	echo -e "\033[1;37mSeu servidor foi configurado"
	echo -e "\033[1;37mPara ver opções digite \033[1;31mVpsPack\033[1;37m_Edição por  \033[1;31muTrevor $0\033[0m"
